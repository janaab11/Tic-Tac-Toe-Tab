{
  "manifest_version": 2,
  "name": "Tic Tac Toe",
  "author": "Kumarmanas Nethil",
  "version": "1.3.0",
  "description": "A Tic Tac Toe game in popup.",
  "icons": {
    "18": "logo_18dp.png",
    "48": "logo_48dp.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action":{
    "default_popup": "index.html",
    "default_title": "Tic Tac Toe"
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'sha256-5O/dYqLrb7ACPE0af4/Ult0w4Q9AKtBs1OerNknJaWc='; object-src 'self'"
}